<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="clr-namespace:FlowOfReason.UI.ViewModels"
             xmlns:avalonia="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="vm:GraphSearchGraphResultItemTemplateViewModel"
             x:Class="FlowOfReason.UI.Views.GraphSearchGraphResultItemTemplate">
    <StackPanel Orientation="Horizontal">
        <Button Name="GoToGraphButton">
            <TextBlock Text="{avalonia:MaterialIconExt Kind=FolderOpen}"></TextBlock>
        </Button>
        <TextBlock Name="GraphNameTextBlock" />
    </StackPanel>
</UserControl>
